<div class="get-car-form">
<h1>Valet Information:</h1>
  <%= form_for @customer do |f| %>
    <h3>Location</h3>
    <%= f.collection_select(:location, Location.all, :id, :name ) %>
    <h3>Valet Ticket No.</h3>
    <%= f.text_field :ticket_no %>
    <h3>Text Message<br>Confirmation No.</h3>
    <%= f.text_field :text_confirmation, value: "" %>
    <br>
    <%= f.submit "I'm Ready" %>
  <% end %>
</div>
